# Copy to `.env` and edit only what you need for your deployment.
# Keep this file in source control, never commit `.env`.

# Optional integrations
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4
BLENDER_PATH=

# Defaults
DEFAULT_WIDTH=10
DEFAULT_HEIGHT=10
DEFAULT_DEPTH=10
DEFAULT_SUPPORT_MODE=HYBRID
DEFAULT_COLLISION_CHECK=True

# Output directories (relative to repo root)
EXPORT_DIR=exports
SAVE_DIR=saves

# Performance/debug
USE_PYEMBREE=True
MAX_BLOCKS_WARNING=5000
DEBUG=False
LOG_LEVEL=INFO
VERBOSE=False
